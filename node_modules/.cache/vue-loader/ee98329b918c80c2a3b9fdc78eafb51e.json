>\r\n      <b-form inline>\r\n        <b-form-input\r\n          id=\"input-with-list\"\r\n          class=\"col-sm-4\"\r\n          placeholder=\"Suche deinen Einzelhändler\"\r\n          v-model=\"filter_name\"\r\n          @click=\"search\"\r\n          list=\"input-list\"\r\n        ></b-form-input>\r\n        <!-- <b-form-datalist id=\"input-list\"></b-form-datalist> -->\r\n\r\n        <b-form-select\r\n          id=\"inline-form-custom-select-pref\"\r\n          class=\"col-sm-4\"\r\n          v-model=\"filter_category\"\r\n          :options=\"options\"\r\n        ></b-form-select>\r\n\r\n        <b-button class=\"col-sm-2 btn-search\" @click=\"search\">Suchen</b-button>\r\n      </b-form>\r\n\r\n      <br />\r\n    </div>\r\n    <br />\r\n    <GetCompanies class=\"company-overview\" />\r\n    <br />\r\n\r\n    <div class=\"container-fluid company-search\">\r\n      <div\r\n        class=\"overview-box\"\r\n        v-for=\"(company, com) in companies\"\r\n        :key=\"com\"\r\n        :class=\"{ 'hide-boxes': filter_name != company.name}\"\r\n      >\r\n        <div v-if=\"filter_category == 'Autohaus'\">\r\n          <h4>{{ company.name }}</h4>\r\n          <h6>{{ company.kategorie }}</h6>\r\n          <table class=\"table\">\r\n            <tr>\r\n              <td>\r\n                <font-awesome-icon icon=\"envelope\" />\r\n              </td>\r\n              <td>{{ company.email }}</td>\r\n            </tr>\r\n            <tr>\r\n              <td>\r\n                <font-awesome-icon icon=\"phone\" />\r\n              </td>\r\n              <td>{{ company.tel }}</td>\r\n            </tr>\r\n            <tr>\r\n              <td>\r\n                <font-awesome-icon icon=\"map-marker-alt\" />\r\n              </td>\r\n              <td>\r\n                {{ company.straße }} {{ company.hnummer }}, {{ company.plz }}\r\n                {{ company.stadt }}\r\n              </td>\r\n              <td></td>\r\n            </tr>\r\n            <tr>\r\n              <td>Montag</td>\r\n              <td>{{ company.montag_von }} - {{ company.montag_bis }} / {{ company.montagM_von }} - {{ company.montagM_bis }}</td>\r\n            </tr>\r\n            <tr>\r\n              <td>Dienstag</td>\r\n              <td>{{ company.dienstag_von }} - {{ company.dienstag_bis }} / {{ company.dienstagM_von }} - {{ company.dienstagM_bis }}</td>\r\n            </tr>\r\n            <tr>\r\n              <td>Mittwoch</td>\r\n              <td>{{ company.mittwoch_von }} - {{ company.mittwoch_bis }} / {{ company.mittwochM_von }} - {{ company.mittwochM_bis }}</td>\r\n            </tr>\r\n            <tr>\r\n              <td>Donnerstag</td>\r\n              <td>{{ company.donnerstag_von }} - {{ company.donnerstag_bis }} / {{ company.donnerstagM_von }} - {{ company.donnerstagM_bis }}</td>\r\n            </tr>\r\n            <tr>\r\n              <td>Freitag</td>\r\n              <td>{{ company.freitag_von }} - {{ company.freitag_bis }} / {{ company.freitagM_von }} - {{ company.freitagM_bis }}</td>\r\n            </tr>\r\n            <tr>\r\n              <td>Samstag</td>\r\n              <td>{{ company.samstag_von }} - {{ company.samstag_bis }} / {{ company.samstagM_von }} - {{ company.samstagM_bis }}</td>\r\n            </tr>\r\n            <tr>\r\n              <td>Sonntag</td>\r\n              <td>{{ company.sonntag_von }} - {{ company.sonntag_bis }} / {{ company.sonntagM_von }} - {{ company.sonntagM_bis }}</td>\r\n            </tr>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport $ from \"jquery\";\r\nimport GetCompanies from \"@/components/GetCompanies\";\r\n\r\nexport default {\r\n  name: \"Uebersicht\",\r\n  components: {\r\n    GetCompanies\r\n  },\r\n  data() {\r\n    return {\r\n      filter_name: \"\",\r\n      filter_category: null,\r\n      companies: \"\",\r\n      options: [\r\n        { value: null, text: \"Kategorie\" },\r\n        {\r\n          label: \"Auto\",\r\n          options: [\r\n            { value: \"Autohaus\", text: \"Autohaus\" },\r\n            { value: \"Autovermietung\", text: \"Autovermietung\" },\r\n            { value: \"KFZ-Werkstatt\", text: \"KFZ-Werkstatt\" }\r\n          ]\r\n        },\r\n        {\r\n          label: \"Bildung\",\r\n          options: [\r\n            { value: \"Schulung\", text: \"Schulung\" },\r\n            { value: \"Training\", text: \"Training\" },\r\n            { value: \"Nachhilfe\", text: \"Nachhilfe\" },\r\n            { value: \"Musikschule\", text: \"Musikschule\" }\r\n          ]\r\n        },\r\n        {\r\n          label: \"Lebensmittel\",\r\n          options: [\r\n            { value: \"Lebensmittel\", text: \"Lebensmittel\" },\r\n            { value: \"Ernährung\", text: \"Ernährung\" },\r\n            { value: \"Agrar\", text: \"Agrar\" }\r\n          ]\r\n        },\r\n        {\r\n          label: \"Gastronomie\",\r\n          options: [{ value: \"Restaurant\", text: \"Restaurant\" }]\r\n        },\r\n        {\r\n          label: \"Hygiene und Drogerie\",\r\n          options: [\r\n            { value: \"Drogerie\", text: \"Drogerie\" },\r\n            { value: \"Kosmetik\", text: \"Kosmetik\" }\r\n          ]\r\n        },\r\n        {\r\n          label: \"Schreibwaren\",\r\n          options: [{ value: \"Buch und Papier\", text: \"Buch und Papier\" }]\r\n        },\r\n        {\r\n          label: \"Sonstiges\",\r\n          options: [{ value: \"Sonstiges\", text: \"Sonstiges\" }]\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    search() {\r\n      $(document).ready(function() {\r\n        $(\".company-overview\").css(\"display\", \"none\");\r\n        $(\".company-search\").css(\"display\", \"flex\");\r\n      });\r\n    }\r\n  },\r\n  mounted() {\r\n    axios\r\n      .get(\"http://localhost:5000/companies\")\r\n      .then(response => {\r\n        this.companies = response.data;\r\n      })\r\n      .catch(error => {\r\n        console.warn(\"ERROR\");\r\n      });\r\n\r\n    if ((this.filter_name = \"\")) {\r\n      $(\".company-overview\").css(\"display\", \"flex\");\r\n      $(\".company-search\").css(\"display\", \"none\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.übersicht {\r\n  padding-top: 5%;\r\n  font-family: Raleway, sans-serif;\r\n}\r\n@media only screen and (max-width: 1200px) {\r\n  .übersicht {\r\n    padding-top: 5%;\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 900px) {\r\n  .übersicht {\r\n    padding-top: 15%;\r\n  }\r\n}\r\n\r\n.container-fluid {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.overview-box {\r\n  flex: 1 0 21%;\r\n  margin: 30px 50px;\r\n  border: 1px solid #204c78;\r\n  border-radius: 5px;\r\n  background-color: #204c78;\r\n  color: white;\r\n  padding: 10px;\r\n  box-shadow: 2px 2px 2px 1px rgba(0, 0, 0, 0.493);\r\n}\r\n\r\n.table td {\r\n  color: white !important;\r\n  text-align: left;\r\n}\r\n\r\n.box {\r\n  border-radius: 5px;\r\n  box-shadow: 1px 1px 5px 1px rgba(0, 0, 0, 0.493);\r\n  padding: 20px;\r\n  margin: 0 15%;\r\n  background: #dc3545;\r\n  color: white;\r\n}\r\ninput,\r\nselect {\r\n  margin-right: 10px !important;\r\n}\r\n.form-inline {\r\n  justify-content: center;\r\n}\r\n\r\n.btn-search {\r\n  background-color: #204c78 !important;\r\n  border-color: #204c78 !important;\r\n}\r\n\r\n.company-search {\r\n  display: none;\r\n}\r\n\r\n.hide-boxes {\r\n  display: none !important;\r\n}\r\n</style>\r\n"]}]}